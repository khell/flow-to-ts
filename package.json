  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/parser": "^7.5.5",
    "@babel/types": "npm:@khell/babel-types@^7.6.3",
    "@babel/generator": "npm:@khell/babel-generator@^7.6.0",
    "@babel/traverse": "npm:@khell/babel-traverse@^7.6.0",
    "commander": "^2.20.0",
    "fs-readdir-recursive": "^1.1.0",
    "glob": "^7.1.3",
    "path": "^0.12.7",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/typescript": "^2.0.0",
    "codecov": "^3.2.0",
    "flow-bin": "^0.107.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-mock-console": "^0.4.2",
    "jest-mock-process": "^1.2.0",
    "pretty-quick": "^1.10.0",
    "tmp": "^0.1.0",
    "typescript": "^3.6.2"
  },
  "resolutions": {
    "@babel/types": "npm:@khell/babel-types@7.6.3"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.js --coveragePathIgnorePatterns=src/flow-to-ts.js",
    "pretty-quick": "prettier src/*.js test/*.js playground/src/*.tsx --write"
