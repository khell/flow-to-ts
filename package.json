  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-mock-console/dist/setupTestFramework.js"
    ]
  }
